    location ^~ /mgunidb {
       root /usr/share;
       index index.php index.html index.htm;
       try_files $uri $uri/ =404;

       location ~ .php$ {
           include fastcgi_params;
           fastcgi_param SCRIPT_FILENAME $request_filename;
           fastcgi_pass unix:/var/run/php/php5.6-fpm.sock;
       }
    }

